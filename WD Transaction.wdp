  type_code : 0
  p_codes :
   -
     code : |1-
      gsRepExe est une chaîne // Répertoire des données
      gsJournalTransaction  est une chaîne // Fichier de transaction
      gnNumInstance est un entier // Numéro de l'instance de l'application
      
      // Numéro de l'instance du programme
      gnNumInstance = LigneCommande("NUMINST")
      
      LOCAL
      // Langue à utiliser
      nNation est un entier	
      nNation = LigneCommande("LANGUE")
      si nNation>0 alors
      	nation(nNation)
      fin
      
      Si EnModeTest() ALORS 
      	// En mode test on renvoie le répertoire d'exécution
      	gsRepExe = completerep(FrepEnCours())
      SINON
      	// En mode exe on renvoie le répertoire de l'exécutable
      	gsRepExe =  completerep(ExeInfo(exeRépertoire))
      FIN
      
      // fichier de transaction
      gsJournalTransaction = gsRepExe+"Transaction.trs"
      
      // Reprise sur panne automatique en cas de transaction interrompue anormalement
      // lors de l'utilisation précédente. (Journal de transaction resté sur disque)
      Si HTransactionInterrompue(gsJournalTransaction) alors
      	si ouinon("Une transaction a été interrompue (la transaction n'a été ni validée, ni annulée)."+RC+"Ceci peut être dû par exemple à une coupure de courant ou à un arrêt brutal de la tâche."+RC+"La dernière opération n'a donc pas été prise en compte."+RC+"Cliquez sur Oui pour annuler automatiquement la transaction interrompue."+RC+"Cliquez sur Non pour annuler la transaction interrompue avec WDTRANS. Lancez l'utilitaire WDTRANS fourni avec WINDEV.") alors
      		HTransactionannule(gsJournalTransaction)	
      	sinon
      		finprogramme()
      	FIN
      FIN
      
      // HF
      HCréationSiInexistant("*","",hOuvertureDifférée)
      
      hmode("*",hModeMulti)
      HSubstRep("?",gsRepExe)
