  procedures :
   -
     name : AjouteElément
     procedure_id : 4378962864456647982
     type_code : 14
     code : |1-
      Procédure AjouteElément(psFruit)
      // Ajoute un fruit au panier
      
      sELON majuscule(psFruit)
      	cas "FRAISE"
      		si (gnFraiseStock-gnFraisePanier)>0 	alors gnFraisePanier++
      	cas "CERISE"
      		si (gnCeriseStock-gnCerisePanier)>0 	alors gnCerisePanier++
      	cas "ORANGE"
      		si (gnOrangeStock-gnOrangePanier)>0	alors gnOrangePanier++
      	cas "POMME"
      		si (gnPommeStock-gnPommePanier)>0 	alors gnPommePanier++
      	CAS "BANANE"
      		si (gnBananeStock-gnBananePanier)>0 	alors gnBananePanier++
      	CAS "RAISIN"
      		si (gnRaisinStock-gnRaisinPanier)>0 	alors gnRaisinPanier++
      FIN
      
      // Construit les libellés en fonction du stock et du panier
      ConstruitLibellés()
     type : 458752
   -
     name : ConstruitLibellés
     procedure_id : 4378962864849077550
     type_code : 14
     code : |1-
      Procédure ConstruitLibellés()
      // Construit les libellés en fonction du stock et du panier
      
      LIB_FRAISE = ChaîneConstruit("Fraises restantes lors de la dernière lecture : %1"+RC+"Fraises restantes si vous validez votre panier : %2",gnFraiseStock,gnFraiseStock-gnFraisePanier)
      LIB_CERISE = ChaîneConstruit("Cerises restantes lors de la dernière lecture : %1"+RC+"Cerises restantes si vous validez votre panier : %2",gnCeriseStock,gnCeriseStock-gnCerisePanier)
      LIB_ANANAS = ChaîneConstruit("Oranges restantes lors de la dernière lecture : %1"+RC+"Oranges restantes si vous validez votre panier : %2",gnOrangeStock,gnOrangeStock-gnOrangePanier)
      LIB_POMME = ChaîneConstruit("Pommes restantes lors de la dernière lecture : %1"+RC+"Pommes restantes si vous validez votre panier : %2",gnPommeStock,gnPommeStock-gnPommePanier)
      LIB_BANANE = ChaîneConstruit("Bananes restantes lors de la dernière lecture : %1"+RC+"Bananes restantes si vous validez votre panier : %2",gnBananeStock,gnBananeStock-gnBananePanier)
      LIB_RAISIN = ChaîneConstruit("Raisins restants lors de la dernière lecture : %1"+RC+"Raisins restants si vous validez votre panier : %2",gnRaisinStock,gnRaisinStock-gnRaisinPanier)
      
      LIB_PANIER = ChaîneConstruit("CONTENU DU PANIER"+RC+RC+"%1 cerises(s)"+RC+"%2 fraise(s)"+RC+"%3 orange"+RC+"%4 pomme(s)"+RC+"%5 banane(s)"+RC+"%6 raisin(s)" + rc +RC + "%7 articles dans le panier",gnCerisePanier,gnFraisePanier,gnOrangePanier,gnPommePanier,gnBananePanier,gnRaisinPanier, (gnCerisePanier+gnFraisePanier+gnOrangePanier+gnPommePanier+gnBananePanier+gnRaisinPanier))
      
      // Si au moins un article dans le panier, on affiche le bouton RAZ
      si (gnCerisePanier+gnFraisePanier+gnOrangePanier+gnPommePanier+gnBananePanier+gnRaisinPanier) > 0
      	BTN_RESTAURE..visible = vrai
      FIN
     type : 458752
   -
     name : Enlève
     procedure_id : 4378962864902292782
     type_code : 14
     code : |1-
      Procédure Enlève(psFruit)
      // Enlève un fruit au panier
      
      SELON majuscule(psFruit)
      	cas "FRAISE"
      		si gnFraisePanier>0 	alors gnFraisePanier--
      	cas "CERISE"
      		si gnCerisePanier>0 	alors gnCerisePanier--
      	cas "ORANGE"
      		si gnOrangePanier>0	alors gnOrangePanier--
      	cas "POMME"
      		si gnPommePanier>0 	alors gnPommePanier--
      	CAS "BANANE"
      		si gnBananePanier>0 	alors gnBananePanier--
      	CAS "RAISIN"
      		si gnRaisinPanier>0 	alors gnRaisinPanier--
      FIN
      
      // Construit les libellés en fonction du stock et du panier
      ConstruitLibellés()
     type : 458752
   -
     name : bAjoutLigneCde
     procedure_id : 4378969839035321817
     type_code : 14
     code : |1-
      Procédure bAjoutLigneCde(psFruit,pnStock,pnPanier)
      // Ajout d'une ligne de commande  et modification du stock
      // psFruit est le produit à ajouter
      // pnStock est la quantité en stock lue
      // pnPanier est la quantité ajoutée au panier
      
      bFlag est un booleen
      bFlag = vrai
      
      // si ce produit a été ajouté au panier
      si pnPanier>0 alors
      	// Vérification du stock
      	HLitRecherchePremier(STOCK,Fruit,psFruit)
      	si htrouve() alors
      		// si la quantité en stock ne permet plus de livrer la quantité demandée
      		si stock.Quantité<pnPanier alors
      			HTransactionAnnule()
      			bFlag=faux
      			Info(chaineconstruit("Lors de la lecture dans le fichier, il restait %1 %2(s). Il n'en reste maintenant que %3 car entre temps un autre utilisateur a également ajouté ce produit à son panier."+RC+"Nous ne pouvons donc pas vous donner satisfaction en vous fournissant la quantité demandée. La commande a été annulée.",...
      								pnStock,psFruit,stock.Quantité))
      								
      			// Lit les quantités en stock
      			LitStock()	
      
      			// Construit les libellés en fonction du stock et du panier
      			ConstruitLibellés()
      
      
      		sinon
      			// Ajout de la ligne de commande
      			LIGNECDE.IDCOMMANDE=COMMANDE.IDCOMMANDE
      			LIGNECDE.IDSTOCK=STOCK.IDSTOCK
      			LIGNECDE.Fruit=psFruit
      			LIGNECDE.Quantité=pnPanier
      			si pas hajoute(LIGNECDE) alors
      				HTransactionAnnule()
      				bFlag=faux
      				erreur("Impossible de créer une ligne de commande.",herreurinfo())
      			FIN
      
      			// Décrémentation du stock
      			stock.Quantité-=pnPanier
      			si pas hmodifie(STOCK) alors
      				HTransactionAnnule()
      				bFlag=faux
      				erreur("Impossible de modifier le stock.",herreurinfo())
      			fin
      		fin
      
      	// si le produit n'existe plus	
      	sinon
      		HTransactionAnnule()
      		bFlag=faux
      		info(ChaîneConstruit("Le produit %1 est introuvable",psFruit))
      	FIN
      FIN
      
      // Compte rendu
      renvoyer bFlag
     type : 458752
   -
     name : LitStock
     procedure_id : 4379014412827355931
