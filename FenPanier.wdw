    name : BTN_VALIDER
    identifier : 0x3cc5338615f6c609
    internal_properties : 
    properties :
     x : 601
     y : 198
     width : 158
     height : 36
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : CAAAAAgAAAD3PfhAj13+e3Tq365W/blzvSGdoN6J5K4US3kiB8bjYfoY0ANQmI+kHwpltaxV/4XJzvToP9zvc0FAUaUEhW3Nud9GGA0xCOiIYt4jowi+e0qmBhgGkQ==
        code : |1-
         si (gnFraisePanier+gnCerisePanier+gnOrangePanier+gnPommePanier+gnBananePanier+gnRaisinPanier)=0 alors
         	info("Votre panier est vide.","Aucune commande n'a été enregistrée.","","Cliquez sur le signe + pour ajouter un fruit au panier.")
         	retour
         FIN
         
         // DEBUT DE TRANSACTION ***********************************************************************************************
         HTransactionDébut(gsRepExe+"Transaction.trs")
         
         // création de la commande
         commande.Date=Datedujour()
         commande.Heure=gauche(Maintenant(),4)
         HAjoute(COMMANDE)
         
         // Ajout des lignes de commande et modification du stock
         si pas bAjoutLigneCde("FRAISE",gnFraiseStock,gnFraisePanier) alors retour
         si pas bAjoutLigneCde("CERISE",gnCeriseStock,gnCerisePanier) alors retour
         si pas bAjoutLigneCde("ORANGE",gnOrangeStock,gnOrangePanier) alors retour
         si pas bAjoutLigneCde("POMME",gnPommeStock,gnPommePanier) alors retour
         si pas bAjoutLigneCde("BANANE",gnBananeStock,gnBananePanier) alors retour
         si pas bAjoutLigneCde("RAISIN",gnRaisinStock,gnRaisinPanier) alors retour
         
         
         // ATTENTION : 	L'APPEL D'UN ECRAN DURANT UNE TRANSACTION N'EST UTILISE QUE POUR LES BESOINS DE L'EXEMPLE
         // 				CECI EST FORTEMENT DECONSEILLE DANS UNE APPLICATION : 
         //				LA TRANSACTION DOIT ETRE LA PLUS COURTE POSSIBLE ET NE DOIT CONTENIR QUE DES OPERATIONS D'ECRITURE INDISSOCIABLES
         SELON Ouvre(Fen_FinTrans)
         	CAS 1
         		// TRANSACTION VALIDEE ****************************************************************************************
         		HTransactionFin()
         		
         		gnFraisePanier = 0
         		gnCerisePanier = 0
         		gnOrangePanier = 0
         		gnPommePanier 	= 0
         		gnBananePanier = 0
         		gnRaisinPanier = 0
         		
         		// Lit les quantités en stock
         		LitStock()
         
         		// Construit les libellés en fonction du stock et du panier
         		ConstruitLibellés()
         		
         		Info("Votre commande a été enregistrée.")
         	CAS 2
         		// TRANSACTION ANNULEE ****************************************************************************************
         		Sablier(Vrai) 
         		HTransactionAnnule()
         		Sablier(Faux)
         		Info("Votre commande a été annulée.")
         	CAS 3
         		// TRANSACTION ANNULEE IMPLICITEMENT **************************************************************************
         		Info("Une division par 0 non-protégée va être provoquée intentionnellement.","Le programme va être arrêté brutalement.","","Le moteur HFSQL va annuler la transaction automatiquement.")
         		x est un entier
         		
         		x = 1/0				// Erreur programme intentionnelle
         	AUTRES CAS
         		// SIMULATION DE PANNE DURANT UNE TRANSACTION *****************************************************************
         		Ouvre(Fen_Panne)	
         		
         		// Dans le cas où l'utilisateur ne souhaite pas redémarrer sa machine
         		// TRANSACTION ANNULEE ****************************************************************************************
         		Sablier(Vrai) 
         		HTransactionAnnule()
         		Sablier(Faux)
         		Info("Votre commande a été annulée.")	
         FIN
         
         
         
         CAS ERREUR:
         	// Annulation de la transaction
         	hTransactionannule()
         	retour
        type : 18
