     code : |1-
      Procédure LitStock()
      // Lit les quantités en stock
      
      HLitRecherchePremier(STOCK,Fruit,"FRAISE")
      si htrouve() alors gnFraiseStock = stock.Quantité
      
      HLitRecherchePremier(STOCK,Fruit,"CERISE")
      si htrouve() alors gnCeriseStock = stock.Quantité
      
      HLitRecherchePremier(STOCK,Fruit,"ORANGE")
      si htrouve() alors gnOrangeStock = stock.Quantité
      
      HLitRecherchePremier(STOCK,Fruit,"POMME")
      si htrouve() alors gnPommeStock = stock.Quantité
      
      HLitRecherchePremier(STOCK,Fruit,"BANANE")
      si htrouve() alors gnBananeStock = stock.Quantité
      
      HLitRecherchePremier(STOCK,Fruit,"RAISIN")
      si htrouve() alors gnRaisinStock = stock.Quantité
      
      gnFraisePanier=0
      gnCerisePanier=0
      gnOrangePanier=0
      gnPommePanier=0
      gnBananePanier=0
      gnRaisinPanier=0
